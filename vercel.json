{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/api/pi/approve",
      "dest": "/api/pi/approve.js"
    },
    {
      "src": "/api/pi/complete",
      "dest": "/api/pi/complete.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}